{{#> base }}
<a href="./">Back</a>
<h1>Boudicca EventCollectors WebUI</h1>

<h2>Single Collection</h2>

<p>Collector: {{singleCollection.name}}</p>
<p>Duration: {{singleCollection.duration}}</p>
<p>Start/End Time: {{singleCollection.startEndTime}}</p>
<p>Errors: {{singleCollection.errorCount}}</p>
<p>Warnings: {{singleCollection.warningCount}}</p>
<p>Total Events Collected: {{singleCollection.totalEventsCollected}}</p>

<h3>Http Calls</h3>
<table>
    <thead>
    <tr>
        <td>Url</td>
        <td>Response Code</td>
        <td>Duration</td>
        <td>Start/End Time</td>
        <td>Post Data (if it is a post request)</td>
    </tr>
    </thead>
    <tbody>
        {{#each httpCalls as |httpCall|}}
            <tr>
                <td>{{httpCall.url}}</td>
                <td>{{httpCall.responseCode}}</td>
                <td>{{httpCall.duration}}</td>
                <td>{{httpCall.startEndTime}}</td>
                <td>{{httpCall.postData}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>

<h3>Log lines</h3>
<pre>{{logs}}</pre>

{{/base}}